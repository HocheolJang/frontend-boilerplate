{
  "name": "frontend-workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9009 -s public",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "cypress:record": "cypress run --record --parallel",
    "test": "react-scripts test --coverage --verbose --watchAll=false && yarn cypress",
    "test:watch": "react-scripts test --coverage --verbose",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint . --cache --ext .ts,.tsx",
    "lint:fix": "tsc --noEmit && eslint . --cache --fix --ext .ts,.tsx",
    "precommit": "tsc --noEmit && lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,scss,css,md}\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "http-status": "^1.4.2",
    "immer": "^5.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1",
    "styled-tools": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/google-map-react": "^1.1.3",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-loader": "^8.0.6",
    "cypress": "3.8.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "intersection-observer": "^0.7.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.13.0",
    "prop-types": "^15.0.0-0",
    "react-docgen-typescript-loader": "^3.4.0",
    "react-scripts": "^3.2.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "core-js": "^3.6.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "husky": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts(x)?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/style.ts",
      "!src/**/*.stories.tsx"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
